#Przekierowanie na bezWWW
Options +FollowSymLinks
RewriteEngine on
DirectoryIndex index.html
RewriteCond %{HTTP_HOST} ^www.jacekmaciejak.pl$ [NC]
RewriteRule ^(.*)$ http://jacekmaciejak.pl/$1 [R=301,L]
ErrorDocument 404 /404.html
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([a-zA-Z-_0-9]+)/([a-zA-Z-_0-9]+)/?$ index.html?page=$1&id=$2 [L]
RewriteRule ^([a-zA-Z-_0-9]+)/?$ index.html?page=$1 [L]













